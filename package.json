{
  "name": "street-client",
  "version": "1.0.0",
  "description": "Dev Update Engine - Automated GitHub activity summaries",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "npx tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prepare": "npx husky",
    "studio": "prisma studio"
  },
  "lint-staged": {
    "**/*": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "keywords": [
    "github",
    "dev-updates",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/auth-app": "^6.1.0",
    "@octokit/rest": "^20.1.1",
    "@prisma/client": "^6.19.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "node-cron": "^3.0.3",
    "oauth-1.0a": "^2.2.6",
    "openai": "^6.16.0",
    "redis": "^5.10.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.10",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "testcontainers": "^11.12.0",
    "ts-jest": "^29.4.6",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
